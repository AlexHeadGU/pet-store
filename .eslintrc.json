{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "eslint:recommended"
    ],
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "ecmaVersion": 12
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "vue/no-multiple-template-root": "off"
    }
}
